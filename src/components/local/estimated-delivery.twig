{% verbatim %}
  <mj-raw>
    {% if fulfillment.estimated_delivery_at %}
  </mj-raw>
{% endverbatim %}

  <mj-section padding="{{ padding | default('0 0 20px') }}">
    <mj-column>
      <mj-text
        align="center"
        color="{{ style.color['grey-dark'] }}"
      >
        You can start your project on:<br />
          
            <strong>{% verbatim %}{{ fulfillment.estimated_delivery_at | date: '%B %-d, %Y' }}{% endverbatim %}</strong>
          
      </mj-text>
    </mj-column>
  </mj-section>

{% verbatim %}
  <mj-raw>
    {% endif %}
  </mj-raw>
{% endverbatim %}